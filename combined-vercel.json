{
  "version": 2,
  "builds": [
    {
      "src": "combined-app.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/combined-app.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/combined-app.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/combined-app.js"
    }
  ]
}
